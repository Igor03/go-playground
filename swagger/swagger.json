{
    "swagger": "2.0",
    "info": {
        "description": "Dummy API for uploading artifacts",
        "title": "Cache API",
        "contact": {},
        "version": "1.0"
    },
    "basePath": "/",
    "paths": {
        "/artifact": {
            "post": {
                "description": "Receives multipart form with key + file",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "text/plain"
                ],
                "summary": "Upload an artifact",
                "parameters": [
                    {
                        "type": "string",
                        "description": "hash/key",
                        "name": "key",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "artifact file",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "bad request",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}